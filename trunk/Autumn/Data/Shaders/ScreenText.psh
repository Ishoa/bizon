Texture2D tFontTexture : register(t0);
SamplerState sFontSampler : register(s0);

struct PSin
{
	float4	Pos : SV_POSITION;
	float4	Color : COLOR0;
	float2	TexCoord : TEXCOORD0;
};


float4 PSMain( PSin Pin ) : SV_Target
{
	return float4(tFontTexture.Sample(sFontSampler, Pin.TexCoord).aaa, 1);
}